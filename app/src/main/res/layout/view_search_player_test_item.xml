<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout5"
        android:background="@drawable/card_201"
        android:paddingTop="23.5dp"
        android:paddingLeft="16.75dp"
        android:paddingRight="15.75dp">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <com.android.ground.ground.custom.CustomCircularImageView
                android:layout_width="98.75dp"
                android:layout_height="99.25dp"
                android:id="@+id/memImage"
                android:src="@mipmap/ic_launcher"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <com.android.ground.ground.custom.CustomCircularImageView
                android:layout_width="26.5dp"
                android:layout_height="26.5dp"
                android:id="@+id/clubImage"
                android:layout_gravity="left|top"
                android:src="@mipmap/ic_launcher" />
        </FrameLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingLeft="21dp">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:layout_width="22.5dp"
                    android:layout_height="23dp"
                    android:id="@+id/managerYN"
                    android:src="@drawable/captain"
                    android:layout_marginRight="11dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="이름"
                    android:id="@+id/memNameCountScr"
                    android:layout_toLeftOf="@+id/button21"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/memImage"
                    android:layout_toEndOf="@+id/memImage"
                    android:textColor="#ffffffff"
                    android:textSize="27sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="나이"
                    android:id="@+id/age"
                    android:textColor="#ffffffff"
                    android:textSize="12sp" />

            </LinearLayout>

            <ImageView
                android:layout_width="47.5dp"
                android:layout_height="15dp"
                android:id="@+id/clubYN"
                android:src="@drawable/match_201"
                android:layout_marginLeft="34dp" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="지역  "
                    android:id="@+id/textView33"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="지역"
                    android:id="@+id/memLocationName"
                    android:layout_toLeftOf="@+id/button21"
                    android:layout_below="@+id/memNameCountScr"
                    android:layout_toRightOf="@+id/memImage"
                    android:layout_toEndOf="@+id/memImage"
                    android:layout_weight="1"
                    android:textColor="#ffffffff"
                    android:textSize="12sp" />
            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_horizontal">

                <CheckBox
                    android:layout_width="15.25dp"
                    android:layout_height="15.25dp"
                    android:id="@+id/memMainDay_Mon"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/text_actor"
                    android:layout_toEndOf="@+id/text_actor"
                    android:checked="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:button="@drawable/check_box_mon" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="선호  "
                    android:id="@+id/text_actor"
                    android:textColor="#ffffffff"
                    android:textSize="12sp" />

                <CheckBox
                    android:layout_width="15.25dp"
                    android:layout_height="15.25dp"
                    android:id="@+id/memMainDay_Tue"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/memMainDay_Mon"
                    android:layout_toEndOf="@+id/memMainDay_Mon"
                    android:checked="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:button="@drawable/check_box_tue" />

                <CheckBox
                    android:layout_width="15.25dp"
                    android:layout_height="15.25dp"
                    android:id="@+id/memMainDay_Wed"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/memMainDay_Tue"
                    android:layout_toEndOf="@+id/memMainDay_Tue"
                    android:checked="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:button="@drawable/check_box_wed" />

                <CheckBox
                    android:layout_width="15.25dp"
                    android:layout_height="15.25dp"
                    android:id="@+id/memMainDay_Thu"
                    android:checked="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:button="@drawable/check_box_thu"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/memMainDay_Wed"
                    android:layout_toEndOf="@+id/memMainDay_Wed" />

                <CheckBox
                    android:layout_width="15.25dp"
                    android:layout_height="15.25dp"
                    android:id="@+id/memMainDay_Fri"
                    android:checked="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:button="@drawable/check_box_fri"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/memMainDay_Thu"
                    android:layout_toEndOf="@+id/memMainDay_Thu" />

                <CheckBox
                    android:layout_width="15.25dp"
                    android:layout_height="15.25dp"
                    android:id="@+id/memMainDay_Sat"
                    android:checked="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:button="@drawable/check_box_sat"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/memMainDay_Fri"
                    android:layout_toEndOf="@+id/memMainDay_Fri" />

                <CheckBox
                    android:layout_width="15.25dp"
                    android:layout_height="15.25dp"
                    android:id="@+id/memMainDay_Sun"
                    android:checked="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:button="@drawable/check_box_sun"
                    android:layout_alignParentTop="true"
                    android:layout_toRightOf="@+id/memMainDay_Sat"
                    android:layout_toEndOf="@+id/memMainDay_Sat" />

            </RelativeLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="memIntro"
                android:id="@+id/memIntro"
                android:textColor="#ffffffff"
                android:textSize="12sp" />

        </LinearLayout>


        <ImageView
            android:layout_width="51.75dp"
            android:layout_height="51.75dp"
            android:id="@+id/button21"
            android:src="@drawable/button201"
            android:focusable="false"
            android:layout_gravity="bottom"
            android:layout_marginBottom="47.25dp" />

    </LinearLayout>

</RelativeLayout>