<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.ground.ground.controller.person.login.SignupFragment"
    android:background="@drawable/back401">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingRight="5px"
        android:fillViewport="false"
        android:id="@+id/scrollView_signup">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="122.5dp"
                android:layout_height="122.5dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:id="@+id/frameLayout3"
                android:layout_marginTop="24.25dp">

                <com.android.ground.ground.custom.CustomCircularImageView
                    android:id="@+id/memImage"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:src="@mipmap/ic_launcher"
                    android:focusable="false"
                android:layout_centerHorizontal="true"
                    android:layout_gravity="center" />

                <ImageView
                    android:layout_width="31.75dp"
                    android:layout_height="31.75dp"
                    android:id="@+id/imageView18"
                    android:layout_gravity="right|bottom"
                    android:src="@drawable/icon403" />
            </FrameLayout>

            <TextView
                android:id="@+id/memName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="memName"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_below="@+id/frameLayout3"
                android:layout_centerHorizontal="true"
                android:textColor="#ffffffff"
                android:textSize="27sp" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/memName"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="23dp">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right|top"
                    android:id="@+id/linearLayout54"
                    android:layout_below="@+id/memName"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="23.25dp">

                    <TextView
                        android:id="@+id/textView45"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="성       별"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_alignTop="@+id/gender"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:textSize="16sp"
                        android:layout_marginRight="15dp" />

                    <RadioGroup
                        android:id="@+id/gender"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:focusable="false"
                        android:layout_below="@+id/memName"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true">

                        <RadioButton
                            android:id="@+id/radioButton"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="남"
                            android:checked="false"
                            android:focusable="false"
                            android:textColor="#ffffffff"
                            android:textSize="16sp"
                            android:button="@drawable/radio_gender"
                            android:layout_marginRight="10dp" />

                        <RadioButton
                            android:id="@+id/radioButton2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:text="여"
                            android:checked="false"
                            android:focusable="false"
                            android:textColor="#ffffffff"
                            android:textSize="16sp"
                            android:button="@drawable/radio_gender" />

                    </RadioGroup>
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout52"
                    android:layout_gravity="bottom"
                    android:layout_below="@+id/linearLayout54"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="23.25dp">

                    <TextView
                        android:id="@+id/textView46"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="나       이"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_below="@+id/gender"
                        android:textColor="#ffffffff"
                        android:textSize="16sp"
                        android:layout_marginRight="15dp" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="111dp"
                        android:layout_height="28dp"
                        android:id="@+id/linearLayoutAge"
                        android:background="@drawable/shape_droptarget">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="연도"
                            android:id="@+id/textView89"
                            android:textColor="#ffffffff"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:layout_gravity="center" />

                        <ImageView
                            android:layout_width="17.5dp"
                            android:layout_height="11dp"
                            android:id="@+id/imageView22"
                            android:src="@drawable/add701"
                            android:layout_gravity="center_vertical|right" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/linearLayout52"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:id="@+id/linearLayout75"
                    android:layout_marginBottom="23.25dp">

                    <TextView
                        android:id="@+id/textView47"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="활동지역"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_below="@+id/linearLayout52"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:textSize="16sp"
                        android:layout_marginRight="15dp" />



                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="28dp"
                        android:layout_below="@+id/textView55"
                        android:background="@drawable/shape_droptarget"
                        android:id="@+id/button_area_search" >

                        <TextView
                            android:layout_width="169.75dp"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="지역 검색"
                            android:id="@+id/textViewArea"
                            android:layout_below="@+id/textView55"
                            android:layout_toRightOf="@+id/button_area_search"
                            android:layout_toEndOf="@+id/button_area_search"
                            android:textColor="#ffffffff"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="16sp"
                            android:singleLine="true" />

                        <ImageView
                            android:layout_width="17dp"
                            android:layout_height="16.75dp"
                            android:id="@+id/imageView19"
                            android:src="@drawable/icon402"
                            android:layout_gravity="center_vertical|right" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/linearLayout53"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:id="@+id/linearLayout76"
                    android:layout_marginBottom="23.25dp">

                    <TextView
                        android:id="@+id/textView50"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="소       개"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_below="@+id/linearLayout53"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:textSize="16sp"
                        android:layout_marginRight="15dp" />

                    <EditText
                        android:id="@+id/memIntro"
                        android:layout_width="169.5dp"
                        android:layout_height="wrap_content"
                        android:imeOptions="actionDone"
                        android:singleLine="true"

                        android:layout_below="@+id/linearLayout53"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_toRightOf="@+id/textView50"
                        android:layout_toEndOf="@+id/textView50"
                        android:textColor="#ffffffff"
                        android:hint="자기소개를 적어주세요(6자 이내)"
                        android:textColorHint="#ffffffff"
                        android:theme="@style/Theme.App.EditText"
                        android:textSize="9sp"/>
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/linearLayout74"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="23.25dp"
                    android:id="@+id/LinearLayout90">

                    <TextView
                        android:id="@+id/textView52"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="본인실력"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_below="@+id/spinner_position"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:textSize="16sp"
                        android:layout_marginRight="15dp" />

                    <Spinner
                        android:layout_width="100.25dp"
                        android:layout_height="30dp"
                        android:id="@+id/spinner_ability"
                        android:layout_alignTop="@+id/textView52"
                        android:layout_toRightOf="@+id/textView52"
                        android:layout_toEndOf="@+id/textView52" />
                </LinearLayout>

                <Button
                    android:id="@+id/button_complete"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="285dp"
                    android:layout_height="38.5dp"
                    android:layout_gravity="center_horizontal|top"
                    android:text="선수 등록하기"
                    android:focusable="false"
                    android:layout_alignParentBottom="false"
                    android:layout_centerHorizontal="true"
                    android:background="#ff3a1d"
                    android:layout_below="@+id/LinearLayout90"
                    android:layout_marginBottom="26dp"
                    android:textColor="#ffffffff" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout53"
                    android:layout_below="@+id/linearLayout75"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="23.25dp">

                    <TextView
                        android:id="@+id/textView48"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="선호요일"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_below="@+id/button_area_search"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:textSize="16sp"
                        android:layout_marginRight="15dp" />

                    <CheckBox
                        android:id="@+id/memMainDay_Mon"
                        android:layout_width="22.75dp"
                        android:layout_height="22.75dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/textView48"
                        android:checked="false"
                        android:focusable="false"
                        android:button="@drawable/check_box_mon_fc" />

                    <CheckBox
                        android:id="@+id/memMainDay_Tue"
                        android:layout_width="22.75dp"
                        android:layout_height="22.75dp"
                        android:layout_alignTop="@+id/memMainDay_Mon"
                        android:layout_toEndOf="@+id/memMainDay_Mon"
                        android:layout_toRightOf="@+id/memMainDay_Mon"
                        android:checked="false"
                        android:focusable="false"
                        android:button="@drawable/check_box_tue_fc" />

                    <CheckBox
                        android:id="@+id/memMainDay_Wed"
                        android:layout_width="22.75dp"
                        android:layout_height="22.75dp"
                        android:layout_alignTop="@+id/memMainDay_Tue"
                        android:layout_toEndOf="@+id/memMainDay_Tue"
                        android:layout_toRightOf="@+id/memMainDay_Tue"
                        android:checked="false"
                        android:focusable="false"
                        android:button="@drawable/check_box_wed_fc" />

                    <CheckBox
                        android:id="@+id/memMainDay_Thu"
                        android:layout_width="22.75dp"
                        android:layout_height="22.75dp"
                        android:layout_alignTop="@+id/memMainDay_Wed"
                        android:layout_toEndOf="@+id/memMainDay_Wed"
                        android:layout_toRightOf="@+id/memMainDay_Wed"
                        android:checked="false"
                        android:focusable="false"
                        android:button="@drawable/check_box_thu_fc" />

                    <CheckBox
                        android:id="@+id/memMainDay_Fri"
                        android:layout_width="22.75dp"
                        android:layout_height="22.75dp"
                        android:layout_alignTop="@+id/memMainDay_Thu"
                        android:layout_toEndOf="@+id/memMainDay_Thu"
                        android:layout_toRightOf="@+id/memMainDay_Thu"
                        android:checked="false"
                        android:focusable="false"
                        android:button="@drawable/check_box_fri_fc" />

                    <CheckBox
                        android:id="@+id/memMainDay_Sat"
                        android:layout_width="22.75dp"
                        android:layout_height="22.75dp"
                        android:layout_alignTop="@+id/memMainDay_Fri"
                        android:layout_toEndOf="@+id/memMainDay_Fri"
                        android:layout_toRightOf="@+id/memMainDay_Fri"
                        android:checked="false"
                        android:focusable="false"
                        android:button="@drawable/check_box_sat_fc" />

                    <CheckBox
                        android:id="@+id/memMainDay_Sun"
                        android:layout_width="22.75dp"
                        android:layout_height="22.75dp"
                        android:layout_alignTop="@+id/memMainDay_Sat"
                        android:layout_toEndOf="@+id/memMainDay_Sat"
                        android:layout_toRightOf="@+id/memMainDay_Sat"
                        android:checked="false"
                        android:focusable="false"
                        android:button="@drawable/check_box_sun_fc" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout74"
                    android:layout_below="@+id/linearLayout76"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="23.25dp">

                    <TextView
                        android:id="@+id/textView51"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="포  지  션"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_below="@+id/memIntro"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:textSize="16sp"
                        android:layout_marginRight="15dp" />

                    <Spinner
                        android:layout_width="100.25dp"
                        android:layout_height="30dp"
                        android:id="@+id/spinner_position"
                        android:layout_below="@+id/memIntro"
                        android:layout_toRightOf="@+id/textView51"
                        android:layout_toEndOf="@+id/textView51" />
                </LinearLayout>

            </RelativeLayout>

        </RelativeLayout>
    </ScrollView>

</FrameLayout>
