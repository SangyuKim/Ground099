<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_your_profile"
    tools:context="com.android.ground.ground.controller.person.profile.YourProfileActivity">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView4" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignTop="@+id/textView63"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <FrameLayout
                    android:layout_width="111.5dp"
                    android:layout_height="111.5dp"
                    android:id="@+id/frameLayout4"
                    android:layout_marginBottom="19.5dp"
                    android:layout_marginRight="30.25dp"
                    android:layout_marginLeft="26.25dp">

                    <com.android.ground.ground.custom.CustomCircularImageView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:id="@+id/memImage"
                        android:src="@mipmap/ic_launcher"
                        android:layout_alignTop="@+id/winLoseDraw"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_gravity="left|top" />

                    <com.android.ground.ground.custom.CustomCircularImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:id="@+id/clubImage"
                        android:layout_alignTop="@+id/memImage"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:src="@mipmap/ic_launcher"
                        />

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:id="@+id/position"
                        android:layout_below="@+id/memImage"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:src="@mipmap/ic_launcher"
                        android:layout_gravity="right|bottom" />

                </FrameLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="뛰는 것이 좋소이다."
                    android:id="@+id/memIntro"
                    android:textColor="#ffffffff"
                    android:layout_below="@+id/frameLayout4"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:background="@color/gray"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:singleLine="true"
                    android:gravity="center"
                    android:textSize="12sp"
                    android:layout_marginRight="6.75dp"
                    android:layout_marginLeft="6.75dp" />

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_toRightOf="@+id/frameLayout4"
                    android:layout_toEndOf="@+id/frameLayout4">

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:id="@+id/managerYN"
                            android:layout_alignTop="@+id/position"
                            android:layout_toRightOf="@+id/textView69"
                            android:layout_toEndOf="@+id/textView69"
                            android:src="@mipmap/ic_launcher" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="김강수(남)"
                            android:id="@+id/memNameGender"
                            android:textColor="#ffffffff"
                            android:layout_alignParentTop="true"
                            android:layout_alignRight="@+id/btnRequest"
                            android:textSize="27sp"
                            android:singleLine="true" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="승패: 3승 2패"
                        android:id="@+id/winLoseDraw"
                        android:layout_alignParentEnd="true"
                        android:textColor="#ffffffff"
                        android:layout_above="@+id/score"
                        android:layout_alignParentRight="true"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="득점: 3골 2도움"
                        android:id="@+id/score"
                        android:textColor="#ffffffff"
                        android:layout_above="@+id/mvp"
                        android:layout_alignParentRight="true"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="MVP: 2회"
                        android:id="@+id/mvp"
                        android:layout_alignParentEnd="true"
                        android:textColor="#ffffffff"
                        android:layout_above="@+id/skill"
                        android:layout_alignParentRight="true"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="실력"
                        android:id="@+id/skill"
                        android:textColor="#ffffffff"
                        android:layout_above="@+id/memIntro"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:textSize="16sp" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="238.75dp"
                android:layout_height="153dp"
                android:id="@+id/relativeLayout5"
                android:background="@drawable/shape_droptarget"
                android:layout_below="@+id/memImage"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="161dp"
                android:layout_marginLeft="6.75dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="선수정보"
                    android:id="@+id/textView68"
                    android:layout_alignParentTop="true"
                    android:textColor="#ffffffff"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:background="@color/gray"
                    android:layout_marginBottom="10dp"
                    android:textSize="12sp" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout79"
                    android:layout_below="@+id/textView68"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="소속:"
                        android:id="@+id/textView69"
                        android:layout_below="@+id/relativeLayout"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:layout_marginRight="10dp"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="신림 FC"
                        android:id="@+id/clubName"
                        android:layout_alignTop="@+id/textView69"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:textColor="#ffffffff"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout78"
                    android:layout_below="@+id/linearLayout79"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="나이:"
                        android:id="@+id/textView71"
                        android:layout_below="@+id/textView69"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:layout_marginRight="10dp"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="26"
                        android:id="@+id/age"
                        android:layout_below="@+id/clubName"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:textColor="#ffffffff"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout77"
                    android:layout_below="@+id/linearLayout78"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="지역:"
                        android:id="@+id/textView73"
                        android:layout_below="@+id/textView71"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:layout_marginRight="10dp"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="서울시 신림"
                        android:id="@+id/memLocationName"
                        android:layout_below="@+id/age"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:textColor="#ffffffff"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/linearLayout77"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="10dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="선호:"
                        android:id="@+id/textView75"
                        android:layout_below="@+id/textView73"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:textColor="#ffffffff"
                        android:layout_marginRight="10dp"
                        android:textSize="16sp" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/linearLayout61"
                        android:layout_below="@+id/textView75"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true">

                        <CheckBox
                            android:layout_width="21.25dp"
                            android:layout_height="21.25dp"
                            android:id="@+id/memMainDay_Mon"
                            android:layout_below="@+id/textView75"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:checked="false"
                            android:button="@drawable/check_box_mon" />

                        <CheckBox
                            android:layout_width="21.25dp"
                            android:layout_height="21.25dp"
                            android:id="@+id/memMainDay_Tue"
                            android:layout_alignTop="@+id/memMainDay_Mon"
                            android:layout_toRightOf="@+id/memMainDay_Mon"
                            android:layout_toEndOf="@+id/memMainDay_Mon"
                            android:checked="false"
                            android:button="@drawable/check_box_tue" />

                        <CheckBox
                            android:layout_width="21.25dp"
                            android:layout_height="21.25dp"
                            android:id="@+id/memMainDay_Wed"
                            android:layout_alignTop="@+id/memMainDay_Tue"
                            android:layout_toRightOf="@+id/memMainDay_Tue"
                            android:layout_toEndOf="@+id/memMainDay_Tue"
                            android:checked="false"
                            android:button="@drawable/check_box_wed" />

                        <CheckBox
                            android:layout_width="21.25dp"
                            android:layout_height="21.25dp"
                            android:id="@+id/memMainDay_Thu"
                            android:layout_alignTop="@+id/memMainDay_Wed"
                            android:layout_toRightOf="@+id/memMainDay_Wed"
                            android:layout_toEndOf="@+id/memMainDay_Wed"
                            android:checked="false"
                            android:button="@drawable/check_box_thu" />

                        <CheckBox
                            android:layout_width="21.25dp"
                            android:layout_height="21.25dp"
                            android:id="@+id/memMainDay_Fri"
                            android:checked="false"
                            android:button="@drawable/check_box_fri" />

                        <CheckBox
                            android:layout_width="21.25dp"
                            android:layout_height="21.25dp"
                            android:id="@+id/memMainDay_Sat"
                            android:checked="false"
                            android:button="@drawable/check_box_sat" />

                        <CheckBox
                            android:layout_width="21.25dp"
                            android:layout_height="21.25dp"
                            android:id="@+id/memMainDay_Sun"
                            android:checked="false"
                            android:button="@drawable/check_box_sun" />
                    </LinearLayout>

                </LinearLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/relativeLayout9"
                android:layout_below="@+id/relativeLayout11"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout7"
                    android:layout_below="@+id/textView5"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:id="@+id/textView86"
                        android:visibility="invisible"
                        android:layout_weight="1" />

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="53dp"
                        android:layout_height="65dp"
                        android:layout_marginRight="25dp"
                        android:id="@+id/oldLayout1">

                        <com.android.ground.ground.custom.CustomCircularImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:id="@+id/oldClubImage1"
                            android:src="@mipmap/ic_launcher"
                            android:visibility="invisible"
                            android:layout_gravity="center_horizontal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="oldClubName1"
                            android:id="@+id/oldClubName1"
                            android:textColor="#ffffff"
                            android:layout_gravity="center_horizontal" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="53dp"
                        android:layout_height="65dp"
                        android:layout_marginRight="25dp"
                        android:id="@+id/oldLayout2">

                        <com.android.ground.ground.custom.CustomCircularImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:id="@+id/oldClubImage2"
                            android:src="@mipmap/ic_launcher"
                            android:visibility="invisible"
                            android:layout_gravity="center_horizontal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Small Text"
                            android:id="@+id/oldClubName2"
                            android:textColor="#ffffff"
                            android:layout_gravity="center_horizontal" />
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="53dp"
                        android:layout_height="65dp"
                        android:id="@+id/oldLayout3">

                        <com.android.ground.ground.custom.CustomCircularImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:id="@+id/oldClubImage3"
                            android:src="@mipmap/ic_launcher"
                            android:visibility="invisible"
                            android:layout_gravity="center_horizontal" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Small Text"
                            android:id="@+id/oldClubName3"
                            android:textColor="#ffffff"
                            android:layout_gravity="center_horizontal" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:id="@+id/textView87"
                        android:visibility="invisible"
                        android:layout_weight="1" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="이적내역"
                    android:id="@+id/textView5"
                    android:textColor="#ffffffff"
                    android:layout_weight="1"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:textSize="16sp" />

            </RelativeLayout>

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="339dp"
                android:layout_height="38.5dp"
                android:text="가입권유"
                android:id="@+id/btnRequest"
                android:layout_below="@+id/relativeLayout9"
                android:layout_centerHorizontal="true"
                android:background="#ff3a1d"
                android:layout_marginTop="20dp" />

            <RelativeLayout
                android:layout_width="100.25dp"
                android:layout_height="73dp"
                android:layout_alignTop="@+id/relativeLayout5"
                android:layout_toRightOf="@+id/relativeLayout5"
                android:layout_toEndOf="@+id/relativeLayout5"
                android:id="@+id/relativeLayout10"
                android:layout_marginRight="6.75dp"
                android:layout_marginBottom="7dp"
                android:layout_marginLeft="7.5dp">

                <ImageView
                    android:layout_width="100.25dp"
                    android:layout_height="73dp"
                    android:id="@+id/btnFc"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:src="@drawable/fcbox501" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="100.25dp"
                android:layout_height="73dp"
                android:layout_below="@+id/relativeLayout10"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:id="@+id/relativeLayout11"
                android:layout_marginRight="6.75dp"
                android:layout_alignLeft="@+id/relativeLayout10"
                android:layout_alignStart="@+id/relativeLayout10">



                <ImageView
                    android:layout_width="100.25dp"
                    android:layout_height="73dp"
                    android:id="@+id/btnMsg"
                    android:src="@drawable/messagebox501" />
            </RelativeLayout>

        </RelativeLayout>
    </ScrollView>


</RelativeLayout>
