<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <!--<ImageView-->
        <!--android:layout_width="30dp"-->
        <!--android:layout_height="30dp"-->
        <!--android:id="@+id/managerYN"-->
        <!--android:layout_alignParentTop="true"-->
        <!--android:layout_toRightOf="@+id/position"-->
        <!--android:layout_toEndOf="@+id/position" />-->


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.android.ground.ground.custom.CustomCircularImageView
            android:layout_width="32.25dp"
            android:layout_height="32.25dp"
            android:id="@+id/position"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical" />

        <com.android.ground.ground.custom.CustomCircularImageView
            android:layout_width="32.25dp"
            android:layout_height="32.25dp"
            android:id="@+id/memImage"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/position"
            android:layout_toEndOf="@+id/position"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical" />

        <TextView
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="김상철"
            android:id="@+id/memName"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/memImage"
            android:layout_toEndOf="@+id/memImage"
            android:textColor="#ffffffff"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:singleLine="true" />

        <com.android.ground.ground.custom.CustomRoundCornerProgressBar
            android:layout_width="80dp"
            android:layout_height="8dp"
            android:id="@+id/progressBarSkill"
            android:maxHeight="10dp"

            android:indeterminate="false"
            android:progressDrawable="@drawable/progress_bar"
            app:rcProgressColor="#ff3a1d"
            android:layout_gravity="center_vertical"

            app:rcBackgroundColor="#66464646"
            app:rcMax="50"
            app:rcProgress="25"
            android:layout_alignBottom="@+id/memName"
            android:layout_toRightOf="@+id/memName"
            android:layout_toEndOf="@+id/memName"
            android:layout_centerVertical="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="0.0"
            android:id="@+id/clubSkill"
            android:textColor="#ffffffff"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/progressBarSkill"
            android:layout_toEndOf="@+id/progressBarSkill"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView90"
            android:visibility="invisible"
            android:layout_weight="1"
            android:layout_gravity="center_vertical" />

        <ImageView
            android:layout_width="17.25dp"
            android:layout_height="16.25dp"
            android:id="@+id/button25"
            android:src="@drawable/icon201"
            android:layout_alignTop="@+id/progressBarSkill"
            android:layout_toLeftOf="@+id/button26"
            android:layout_toStartOf="@+id/button26"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="5dp" />

        <ImageView
            android:layout_width="23.25dp"
            android:layout_height="16dp"
            android:id="@+id/button26"
            android:src="@drawable/button04"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="5dp" />


    </LinearLayout>
</RelativeLayout>
